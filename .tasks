# vim: set fenc=utf-8 ft=dosini:

# define a new task named "file-build"
[file-build-run-test]
command=make && ./parallel-divisor-function ./data/entrada.txt ./data/entrada_out.txt && python3 ./test/df.py ./data/entrada.txt ./data/entrada_out.txt
cwd=$(VIM_ROOT)
output=quickfix
errorformat=%f:%l:%m
save=1

[file-build-run-original]

# shell command, use quotation for filenames containing spaces
# check ":AsyncTaskMacro" to see available macros
command=make && ./parallel-divisor-function ./data/entrada.txt ./data/entrada_out.txt

# working directory, can change to $(VIM_ROOT) for project root
cwd=$(VIM_ROOT)

# output mode, can be one of quickfix and terminal
# - quickfix: output to quickfix window
# - terminal: run the command in the internal terminal
output=quickfix

# this is for output=quickfix only
# if it is omitted, vim's current errorformat will be used.
errorformat=%f:%l:%m

# save file before execute
save=1

[file-build-run]

# shell command, use quotation for filenames containing spaces
# check ":AsyncTaskMacro" to see available macros
command=make && ./parallel-divisor-function ./data/t1.txt ./data/t1_out.txt

# working directory, can change to $(VIM_ROOT) for project root
cwd=$(VIM_ROOT)

# output mode, can be one of quickfix and terminal
# - quickfix: output to quickfix window
# - terminal: run the command in the internal terminal
output=quickfix

# this is for output=quickfix only
# if it is omitted, vim's current errorformat will be used.
errorformat=%f:%l:%m

# save file before execute
save=1


